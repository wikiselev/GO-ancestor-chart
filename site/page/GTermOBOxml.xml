<obo xmlns:page='java:uk.ac.ebi.quickgo.web.servlets.GTermServlet.TermPage'
     xmlns:list='java:uk.ac.ebi.interpro.jxbp2.ListContent'
     xmlns:relation='java:uk.ac.ebi.quickgo.web.data.TermRelation'
     xmlns:term='java:uk.ac.ebi.quickgo.web.data.Term'
     xmlns:xr="java:uk.ac.ebi.quickgo.web.data.Term.XRef">

	<header>
		<format-version>1.2</format-version>
		<auto-generated-by>QuickGO: http://www.ebi.ac.uk/QuickGO</auto-generated-by>
		<synonymtypedef>
			<id>systematic_synonym</id>
			<name>Systematic synonym</name>
			<scope>EXACT</scope>
		</synonymtypedef>
		<default-namespace>gene_ontology</default-namespace>
		<remark>This is not the master copy of the OBO files, see http://www.geneontology.org/GO.downloads.ontology.shtml</remark>
	</header>
	<term>
		<id>{term:id}</id>
		<name>{term:name}</name>
		<namespace>{term:namespace}</namespace>
		<def>
			<defstr>{term:definition}</defstr>
		</def>
		<term:hasComment>
			<comment>{term:comment}</comment>
		</term:hasComment>

		<term:synonyms xmlns:synonym='java:uk.ac.ebi.quickgo.web.data.Synonym'>
			<synonym scope="{synonym:type}">
				<synonym_text>{synonym:name}</synonym_text>
			</synonym>
		</term:synonyms>

		<term:altIds list:body='any'>
			<term:altIds xmlns:xr="java:uk.ac.ebi.quickgo.web.data.Term.XRef">
				<alt_id>
					{xr:id}
				</alt_id>
			</term:altIds>
		</term:altIds>

		<term:obsolete>
			<is_obsolete>1</is_obsolete>
		</term:obsolete>
		<term:replacedBy list:body='any'>
			<term:replacedBy>
				<replaced_by>{term:id}</replaced_by>
			</term:replacedBy>
		</term:replacedBy>
		<term:consider list:body='any'>
			<term:consider>
				<consider>{term:id}</consider>
			</term:consider>
		</term:consider>

		<term:xrefs>
			<xref>
				<acc>{xr:id}</acc>
				<dbname>{xr:db}</dbname>
			</xref>
		</term:xrefs>

		<term:isa list:body='any'>
			<term:isa>
				<is_a>
					<relation:parent>{term:id}</relation:parent>
				</is_a>
			</term:isa>
		</term:isa>

		<term:otherParents list:body='any'>
			<term:otherParents>
				<relationship>
					<type>
						<relation:typeof xmlns:rt="java:uk.ac.ebi.quickgo.web.data.RelationType">
							{rt:formalCode}							
						</relation:typeof>
					</type>
					<to><relation:parent>{term:id}</relation:parent></to>
				</relationship>
			</term:otherParents>
		</term:otherParents>
	</term>
</obo>